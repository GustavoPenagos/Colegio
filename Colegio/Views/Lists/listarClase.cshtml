
@{
    ViewBag.Title = "Lista de clase";
}

<main aria-labelledby="title">
    <h2 id="title">@ViewBag.Title</h2>
    <h3>@ViewBag.Message</h3>

    <br />
    <div>
        <table class="table table-striped-columns">
            <thead>
                <tr>
                    <td>Año <br />academico </td>
                    <td>Identificacion<br />Alumno</td>
                    <td>Nombre<br />Alumno </td>
                    <td>Identificacion<br />Profesor</td>
                    <td>Profesor</td>
                    <td>Asignatura</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>2023</td>
                        <td>@item.Id_Alumno</td>
                        <td>@item.Nombre_Alumno</td>
                        <td>@item.Id_Profesor</td>
                        <td>@item.Nombre_Profesor</td>
                        <td>@item.Asignatura</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</main>